{
  "name": "wills-app-starter",
  "description": "A simple app starter app for all the things.",
  "private": true,
  "author": "Will Schoenberger",
  "version": "0.0.1",
  "repository": "",
  "scripts": {
    "start": "brunch watch --server",
    "dev": "yarn start",
    "build": "brunch build --production",
    "server": "node ./server/server",
    "server:dev": "env $(cat .env) nodemon server/server --watch",
    "test": "NODE_ENV=test tape -1 -r babel-register \"app/src/**/*.spec.js\" | tap-notify | tap-spec",
    "testnyan": "NODE_ENV=test tape -1 -r babel-register \"app/src/**/*.spec.js\" | tap-notify | tnyan",
    "test:watch": "npm-watch test",
    "test:cover": "nyc yarn test",
    "lint": "standard --verbose | snazzy",
    "lint:fix": "standard --fix | snazzy",
    "lint:watch": "npm-watch lint",
    "cypress:open": "env $(cat .env) cypress open",
    "watch": "npm-watch",
    "storybook": "start-storybook -s ./app/assets -p 9001 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o public/storybook",
    "init:githooks": "git config core.hooksPath .githooks",
    "heroku-postbuild": "yarn build && yarn storybook:build"
  },
  "watch": {
    "test": {
      "patterns": "./app",
      "extensions": "js",
      "quiet": true,
      "inherit": true
    },
    "lint": {
      "patterns": "./",
      "extensions": "js",
      "quiet": true,
      "inherit": true
    }
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "it",
      "cy",
      "Cypress",
      "expect"
    ]
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "app/src/Icons",
      "public"
    ],
    "cache": true
  },
  "dependencies": {
    "@hyfn/h8-auth": "^1.0.19",
    "@storybook/addon-console": "^1.0.4",
    "@storybook/addon-info": "^3.4.3",
    "@storybook/addons": "^3.4.3",
    "@storybook/react": "^3.4.3",
    "apollo-cache-inmemory": "^1.2.5",
    "apollo-client": "^2.2.8",
    "apollo-link-context": "^1.0.8",
    "apollo-link-http": "^1.5.4",
    "apollo-server-express": "^1.3.5",
    "axios": "^0.18.0",
    "babel-brunch": "~6.1.1",
    "babel-eslint": "^8.2.3",
    "babel-plugin-emotion": "^9.1.2",
    "babel-plugin-inline-dotenv": "^1.1.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "~6.24",
    "basic-auth": "^2.0.0",
    "body-parser": "^1.18.2",
    "brunch": "^2",
    "common-tags": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.6.1",
    "emotion": "^9.1.2",
    "emotion-theming": "^9.1.2",
    "express": "^4.16.3",
    "facepaint": "^1.2.1",
    "graphql": "^14.0.0-rc.2",
    "graphql-tag": "^2.9.2",
    "graphql-tools": "^2.24.0",
    "humps": "^2.0.1",
    "immutable": "3.8.2",
    "jsenv": "^0.0.1",
    "jsenv-brunch": "^2.0.0",
    "lodash": "^4.17.10",
    "moment": "^2.29.4",
    "polished": "^1.9.2",
    "prop-types": "^15.6.0",
    "raven-for-redux": "^1.3.1",
    "raven-js": "^3.26.3",
    "react": "^16.2",
    "react-apollo": "^2.1.4",
    "react-collapse": "^4.0.3",
    "react-datetime": "^2.14.0",
    "react-dom": "^16.2",
    "react-emotion": "^9.1.2",
    "react-jsonschema-form": "^1.0.3",
    "react-modal": "^3.4.4",
    "react-motion": "^0.5.2",
    "react-redux": "~5.0.6",
    "react-select": "^1.2.1",
    "react-tooltip": "^3.6.1",
    "recompose": "^0.27.0",
    "redux": "~3.7.2",
    "redux-immutable": "^4.0.0",
    "redux-little-router": "^15.1.0",
    "redux-saga": "^0.16.0",
    "redux-saga-routines": "^2.2.3",
    "reselect": "^3.0.1",
    "serve-favicon": "^2.5.0",
    "storybook-readme": "^3.3.0",
    "tinycolor2": "^1.4.1",
    "tsscmp": "^1.0.5",
    "uglify-js-brunch": "^2"
  },
  "devDependencies": {
    "auto-reload-brunch": "^2",
    "cypress": "^3.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.16.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "hmr-brunch": "^0.1",
    "nodemon": "^1.17.5",
    "npm-watch": "^0.3.0",
    "nyc": "^11.8.0",
    "sinon": "^6.0.0",
    "snazzy": "^7.1.1",
    "standard": "^11.0.1",
    "tap": "^11.1.4",
    "tap-notify": "^1.0.0",
    "tap-nyan": "^1.1.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.9.0"
  }
}
