{
  "env": {
    "development": {
      "presets": ["env", "react"],
      "plugins": [
        "transform-object-rest-spread",
        'transform-export-extensions',
        ["emotion", { "sourceMap": true, "autoLabel": true }]
      ]
    },
    "test": {
      "presets": ["env", "react"],
      "plugins": [
        "transform-object-rest-spread",
        'transform-export-extensions',
        ["emotion", { "sourceMap": true, "autoLabel": true }],
        ["module-resolver", {
          "root": ["./app"],
          "alias": {}
        }]
      ]
    }
  }
}
